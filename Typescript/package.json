{
  "name": "speedtest-analyzer",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Multiple Typescript scripts utilities",
  "author": "Dylan Tavares <d.tavares@argolight.com>",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "start:error-interpretor": "nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm src/libs/customErrorInterpretor.ts",
    "start:network-analyzer": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/libs/networkAnalytic.ts",
    "start:report-aggregator": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/libs/reportAggregator.ts",
    "vite-build": "vite build",
    "dev:web": "vite",
    "dev:electron": "NODE_ENV=development electron .",
    "start": "vite build && electron .",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "@cloudflare/speedtest": "^1.3.0",
    "@eslint/eslintrc": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "echarts": "^5.5.1",
    "electron": "^31.3.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.40",
    "preline": "^2.4.1",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vite": "^5.3.5",
    "vue": "^3.4.34",
    "vue-echarts": "^7.0.0-beta.0",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.29"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "tsc": "^2.0.4",
    "typescript-eslint": "^7.16.1",
    "vite-plugin-eslint": "^1.8.1"
  }
}
